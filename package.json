    "@tsconfig/node20": "^20.1.2",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/nunjucks": "^3.2.6",
    "@types/prompts": "^2.4.9",
    "chalk": "^5.3.0",
    "esbuild": "^0.18.20",
